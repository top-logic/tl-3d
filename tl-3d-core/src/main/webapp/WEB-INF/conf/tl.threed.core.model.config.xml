<?xml version="1.0" encoding="utf-8" ?>

<application>
	<services>
		<config service-class="com.top_logic.util.model.ModelService">
			<instance>
				<declarations>
					<declaration file="/WEB-INF/model/tl.threed.core.model.xml"/>
				</declarations>
			</instance>
		</config>

		<config service-class="com.top_logic.basic.util.ResourcesModule">
			<instance>
				<bundles>
					<bundle name="model.tl.threed.core.messages"/>
				</bundles>
			</instance>
		</config>

		<config service-class="com.top_logic.model.annotate.util.AttributeSettings">
			<instance class="com.top_logic.element.meta.AttributeSettings">
				<attribute-settings>
					<attribute-setting
						config-name="TRANSFORMATION"
						legacy-type-code="1"
					>
						<field-provider>
							<impl class="com.top_logic.element.meta.form.fieldprovider.format.FormattedFieldProvider"
								control-provider="com.top_logic.layout.form.template.DefaultFormFieldControlProvider"
								format="com.top_logic.threed.core.math.format.TransformationFormat"
							/>
						</field-provider>
					</attribute-setting>
				</attribute-settings>
			</instance>
		</config>
	</services>

	<configs/>
</application>