<?xml version="1.0" encoding="utf-8"?>

<application xmlns:config="http://www.top-logic.com/ns/config/6.0">
	<services>
		<config service-class="com.top_logic.basic.util.ResourcesModule">
			<instance>
				<bundles>
					<bundle name="tl-3d-core-messages" />
				</bundles>
			</instance>
		</config>
		
		<config service-class="com.top_logic.gui.JSFileCompiler">
			<instance>
				<additional-files>
				</additional-files>
			</instance>
		</config>
		
		<config service-class="com.top_logic.model.search.expr.config.SearchBuilder">
			<instance>
				<methods>
					<method name="transform" class="com.top_logic.threed.core.math.script.TransformationConstructor$Builder"/>
					<method name="composeTransformation" class="com.top_logic.threed.core.math.script.Compose$Builder"/>
					<method name="identityTransformation" class="com.top_logic.threed.core.math.script.Identity$Builder"/>
					<method name="rotation" class="com.top_logic.threed.core.math.script.Rotation$Builder"/>
					<method name="rotationX" class="com.top_logic.threed.core.math.script.RotationX$Builder"/>
					<method name="rotationY" class="com.top_logic.threed.core.math.script.RotationY$Builder"/>
					<method name="rotationZ" class="com.top_logic.threed.core.math.script.RotationZ$Builder"/>
					<method name="translation" class="com.top_logic.threed.core.math.script.Translation$Builder"/>
					<method name="inverseTransformation" class="com.top_logic.threed.core.math.script.Inverse$Builder"/>
				</methods>
			</instance>
		</config>
		
		<config service-class="com.top_logic.knowledge.service.migration.MigrationService">
			<instance>
				<modules>
					<module name="tl-3d-core" />
				</modules>
			</instance>
		</config>
	</services>

	<configs>
	</configs>
</application>
